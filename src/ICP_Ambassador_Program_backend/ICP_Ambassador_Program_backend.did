type Admin = record {
  role : AdminRole;
  spaces : vec text;
  wallet_id : principal;
};
type AdminRole = variant { HubLeader; SuperAdmin };
type Benefactors = record { benefactors : vec text; user : text };
type CreateMission = record {
  max_users_rewarded : nat64;
  status : MissionStatus;
  reward : nat64;
  title : text;
  pool : nat64;
  description : text;
  end_date : text;
  start_date : text;
  reward_currency : RewardCurrency;
  space_id : text;
};
type CreateSpace = record {
  chain : text;
  name : text;
  slug : text;
  description : text;
  conversion : nat16;
};
type Errors = variant {
  ErrorUpdatingAdmin;
  NoSpaceFound;
  ErrUpdatingMissionCount;
  NoSubmissionFound;
  NotRegisteredAsAdmin;
  NotASuperAdmin;
  ReferrerNotFound;
  SubmissionAlreadyReviewed;
  ErrUpdatingSubmission;
  SpaceUpdateError;
  ErrUpdatingMission;
  NotOwnerOrSuperAdmin;
  MissionNotFound;
  MissionInActive;
  AlreadyAdmin;
  AlreadySuperAdmin;
  NoUserFound;
};
type Mission = record {
  img : opt text;
  max_users_rewarded : nat64;
  status : MissionStatus;
  tasks : vec Tasks;
  reward : nat64;
  title : text;
  owner : principal;
  pool : nat64;
  description : text;
  end_date : text;
  start_date : text;
  reward_currency : RewardCurrency;
  space_id : text;
  mission_id : text;
};
type MissionStatus = variant { UnActive; Active; Draft };
type ProfileImageData = record { content : opt blob };
type Result = variant { Ok; Err : Errors };
type Result_1 = variant { Ok : text; Err : text };
type Result_10 = variant { Ok : Space; Err : Errors };
type Result_11 = variant { Ok : Submission; Err : Errors };
type Result_2 = variant { Ok : opt Space; Err : Errors };
type Result_3 = variant { Ok : Admin; Err : Errors };
type Result_4 = variant { Ok : vec text; Err : Errors };
type Result_5 = variant { Ok : vec Mission; Err : Errors };
type Result_6 = variant { Ok : vec record { text; Space }; Err : Errors };
type Result_7 = variant { Ok : vec principal; Err : Errors };
type Result_8 = variant { Ok : Benefactors; Err : text };
type Result_9 = variant { Ok : Mission; Err : Errors };
type RewardCurrency = variant { ICP; CKBTC; CKETH };
type Space = record {
  owner : principal;
  shor_description : opt text;
  chain : text;
  logo : opt text;
  name : text;
  slug : text;
  urls : SpaceURLs;
  description : text;
  mission_count : nat16;
  bg_css : opt text;
  bg_img : opt text;
  conversion : nat16;
  space_id : text;
};
type SpaceURLs = record {
  twitter : opt text;
  website : opt text;
  discord : opt text;
  telegram : opt text;
  github : opt text;
  medium : opt text;
};
type Submission = record {
  status : SubmissionStatus;
  user : text;
  tasks_submitted : vec TaskSubmitted;
  submission_id : text;
  mission_id : text;
};
type SubmissionArr = record { mission : text; submissions : vec text };
type SubmissionStatus = variant { Approved; Rejected; Unread };
type TaskSubmitted = variant {
  SendImage : record { id : nat8; img : text };
  SendUrl : record { id : nat8; url : text };
  SendText : record { id : nat8; "text" : text };
  SendTwitterPost : record { id : nat8; post : text };
};
type Tasks = variant {
  SendImage : record { id : nat8; img : text; title : text; body : text };
  SendUrl : record { id : nat8; title : text; body : text };
  SendText : record {
    id : nat8;
    title : text;
    validation_rule : text;
    sample : text;
    body : text;
    max_len : nat16;
  };
  SendTwitterPost : record { id : nat8; title : text; body : text };
};
type UserLevel = variant { GrandMaster; Initiate; Knight; Padawan; Master };
type UserProfile = record {
  hub : text;
  username : text;
  redeem_points : nat64;
  xp_points : nat64;
  referred_by : opt text;
  direct_refers : vec text;
  level : UserLevel;
  discord_id : text;
  wallet : opt principal;
};
service : {
  add_admin_by_super_admin : (principal) -> (Result);
  add_or_update_submission : (Submission) -> (Result);
  add_wallet : (text) -> (Result_1);
  approve_submission : (text) -> (Result_1);
  create_draft_mission : (text) -> (Result);
  create_mission : (CreateMission) -> (Result);
  create_space : (CreateSpace) -> (Result_2);
  create_user : (text, text, text, opt text) -> (Result_1);
  edit_mission : (Mission) -> (Result);
  get_admin : () -> (Result_3) query;
  get_admin_by_principal : (principal) -> (Result_3) query;
  get_all_admin_spaces : () -> (Result_4) query;
  get_all_mission_submissions : (text) -> (opt SubmissionArr) query;
  get_all_space_missions : (text) -> (Result_5) query;
  get_all_spaces : () -> (Result_6) query;
  get_all_super_admins : () -> (Result_7) query;
  get_all_user_benefactors : (text) -> (Result_8);
  get_mission : (text) -> (Result_9) query;
  get_space : (text) -> (Result_10) query;
  get_submission : (text) -> (Result_11) query;
  get_user_data : (text) -> (opt UserProfile) query;
  promote_to_super_admin : (principal) -> (Result);
  register_admin : () -> (Result);
  reject_submission : (text) -> (Result_1);
  update_space : (Space) -> (Result);
  upload_profile_image : (text, ProfileImageData) -> (Result_1);
  withdraw_points : (text, nat64) -> (Result_1);
}
