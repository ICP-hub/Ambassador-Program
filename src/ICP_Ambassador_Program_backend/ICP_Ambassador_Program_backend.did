type Admin = record {
  role : AdminRole;
  spaces : vec text;
  wallet_id : principal;
};
type AdminErrors = variant {
  ErrorUpdatingAdmin;
  NoSpaceFound;
  NotRegisteredAsAdmin;
  NotASuperAdmin;
  SpaceUpdateError;
  NotOwnerOrSuperAdmin;
  AlreadyAdmin;
  AlreadySuperAdmin;
};
type AdminRole = variant { HubLeader; SuperAdmin };
type CreateSpace = record {
  chain : text;
  name : text;
  slug : text;
  description : text;
};
type Result = variant { Ok; Err : AdminErrors };
type Result_1 = variant { Ok; Err : UserErrors };
type Result_2 = variant { Ok : opt Space; Err : AdminErrors };
type Result_3 = variant { Ok : Admin; Err : AdminErrors };
type Result_4 = variant { Ok : vec text; Err : AdminErrors };
type Result_5 = variant { Ok : vec record { text; Space }; Err : AdminErrors };
type Result_6 = variant { Ok : vec principal; Err : AdminErrors };
type Result_7 = variant { Ok : Space; Err : AdminErrors };
type Result_8 = variant { Ok : text; Err : UserErrors };
type Space = record {
  owner : principal;
  shor_description : opt text;
  chain : text;
  logo : opt text;
  name : text;
  slug : text;
  urls : SpaceURLs;
  description : text;
  bg_css : opt text;
  bg_img : opt text;
  space_id : text;
};
type SpaceURLs = record {
  twitter : opt text;
  website : opt text;
  discord : opt text;
  telegram : opt text;
  github : opt text;
  medium : opt text;
};
type UserErrors = variant { NoUserFound };
type UserLevel = variant { GrandMaster; Initiate; Knight; Padawan; Master };
type UserProfile = record {
  username : text;
  redeem_points : nat64;
  xp_points : nat64;
  user_id : text;
  level : UserLevel;
  referral_code : opt text;
  discord_id : text;
  wallet : opt principal;
};
service : () -> {
  add_admin_by_super_admin : (principal) -> (Result);
  add_points : (text, nat64) -> (Result_1);
  create_space : (CreateSpace) -> (Result_2);
  create_user : (text, text, opt text, opt principal) -> (Result_1);
  get_admin : () -> (Result_3) query;
  get_admin_by_principal : (principal) -> (Result_3) query;
  get_all_admin_spaces : () -> (Result_4) query;
  get_all_spaces : () -> (Result_5) query;
  get_all_super_admins : () -> (Result_6) query;
  get_space : (text) -> (Result_7) query;
  get_user : (text) -> (opt UserProfile) query;
  get_user_level : (text) -> (opt record { UserLevel; nat64; nat64 }) query;
  promote_to_super_admin : (principal) -> (Result);
  redeem_points_for_icp : (text, nat64) -> (Result_8);
  register_admin : () -> (Result);
  update_space : (Space) -> (Result);
  update_user : (text, text, opt text, opt principal) -> (Result_1);
}
